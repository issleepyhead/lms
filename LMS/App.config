<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="LMS.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Memory" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.1.2" newVersion="4.0.1.2" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <userSettings>
    <LMS.My.MySettings>
      <setting name="server_name" serializeAs="String">
        <value>localhost</value>
      </setting>
      <setting name="server_password" serializeAs="String">
        <value>password</value>
      </setting>
      <setting name="server_username" serializeAs="String">
        <value>root</value>
      </setting>
      <setting name="connection_string" serializeAs="String">
        <value />
      </setting>
      <setting name="server_database" serializeAs="String">
        <value>lmsdb</value>
      </setting>
      <setting name="server_port" serializeAs="String">
        <value>3306</value>
      </setting>
      <setting name="remember_server" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="user_id" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="last_run" serializeAs="String">
        <value />
      </setting>
      <setting name="session_login" serializeAs="String">
        <value />
      </setting>
      <setting name="remember_user" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="debug_mode" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="user_username" serializeAs="String">
        <value />
      </setting>
      <setting name="email" serializeAs="String">
        <value>example@gmail.com</value>
      </setting>
      <setting name="email_pass" serializeAs="String">
        <value>mypass</value>
      </setting>
      <setting name="spenalty" serializeAs="String">
        <value>10</value>
      </setting>
      <setting name="fpenalty" serializeAs="String">
        <value>10</value>
      </setting>
      <setting name="gpenalty" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="fdays" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="sdays" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="scount" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="fcount" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="return_message" serializeAs="String">
        <value>Dear @borrower,

This is to confirm the return of the following books on @return_date:
@borrowed_books

Thank you for returning them on time. We look forward to serving you again.

Best regards,
Librarian: @librarian</value>
      </setting>
      <setting name="borrow_message" serializeAs="String">
        <value>Dear @borrower,

You borrowed the following books on @borrow_date, due by @overdue_date:
@borrowed_books

Please return them on time.

Thank you,
Librarian: @librarian</value>
      </setting>
      <setting name="overdue_message" serializeAs="String">
        <value>Dear @borrower,

The following books borrowed on @borrow_date are overdue since @overdue_date:
@borrowed_books

Kindly return them promptly.

Thank you,
Librarian: @librarian</value>
      </setting>
      <setting name="enable_notif" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="before_overdue_message" serializeAs="String">
        <value>Dear @borrower,

This is a friendly reminder that the following books borrowed on @borrow_date are due soon:
@borrowed_books

Due Date: @overdue_date

Please return them by the due date to avoid overdue fines.

Thank you,
Librarian: @librarian</value>
      </setting>
      <setting name="backup_folder" serializeAs="String">
        <value />
      </setting>
    </LMS.My.MySettings>
  </userSettings>
</configuration>